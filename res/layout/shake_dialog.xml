<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/restore_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:gravity="center_horizontal"
        android:layout_marginTop="@dimen/shake_dialog_margin_top"
        android:layout_marginBottom="@dimen/shake_dialog_margin_item"
        android:layout_marginLeft="@dimen/shake_dialog_margin_left"
        android:layout_marginRight="@dimen/shake_dialog_margin_right"
        android:textSize="@dimen/browsersetting_cell_text_size" />
    <TextView
        android:id="@+id/restore_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:gravity="center_horizontal"
        android:textColor="@color/shake_dialog_url"
        android:layout_marginBottom="@dimen/shake_dialog_margin_bottom"
        android:layout_marginLeft="@dimen/shake_dialog_margin_left"
        android:layout_marginRight="@dimen/shake_dialog_margin_right"
        android:textSize="@dimen/browsersetting_cell_text_size" />
</LinearLayout>

