<?xml version="1.0" encoding="utf-8"?>
<com.android.myapidemo.smartisan.navigation.NavigationLeftView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nav_left_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.android.myapidemo.smartisan.navigation.DynamicGridView
        android:id="@+id/dynamic_grid"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:listSelector="@android:color/transparent"
        android:paddingLeft="6dp"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:paddingRight="6dp"
        android:scrollbars="none" />

</com.android.myapidemo.smartisan.navigation.NavigationLeftView>